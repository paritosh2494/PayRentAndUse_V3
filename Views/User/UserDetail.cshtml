@model IEnumerable<PayRentAndUse_V3.Models.UserClass>
@{
    ViewBag.Title = "UserDetail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>User Booking Details</h2>

<table id="user" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>User Name</th>
            <th>Gender</th>
            <th>Email Id</th>
            <th>Date Added</th>
            <th>Action</th>
            <th>Feedback</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
        <tr>
            <td>@user.Name</td>
            <td>@user.Gender</td>
            <td>@user.Email</td>
            <td>@user.DateAdded.ToLongDateString()</td>
            <td>@Html.ActionLink("Booking Detail", "BookingDetail", "User", new { id = user.Id }, null) </td>
            <td>@Html.ActionLink("User Feedback", "FeedbackDetail", "User", new { id = user.Id }, null) </td>
        </tr>
        }
    </tbody>
</table>