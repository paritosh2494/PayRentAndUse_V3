@model IEnumerable<PayRentAndUse_V3.Models.FeedbackClass>
@{
    ViewBag.Title = "FeedbackDetail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@if (Model.Count() > 0)
{
    <h2>@ViewData["UserName"] Feedbacks</h2>
    foreach (var (Feedback, index) in Model.Select((value, i) => (value, i)))
    {
        <h3>Feedback @(index + 1)</h3>
        <ul>
            <li><b>Rating : </b>@Feedback.Rating </li>
            <li><b>Feedback Comments : </b> @Feedback.FeedBack</li>
        </ul>
    }
}
else
{
    <h2>No Feedbacks for user @ViewData["UserName"] </h2>
}



